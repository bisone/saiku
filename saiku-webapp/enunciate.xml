		<include pattern="org.saiku.web.rest.**" />
	</api-classes>
	<api-import pattern="org.saiku.web.**" />

	<webapp mergeWebXML="src/main/web/web.xml" />

	<modules>

		<docs>
			<war docsDir="docs" />
		</docs>
		<spring-app enableSecurity="false">
